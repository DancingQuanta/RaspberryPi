sudo mkdir /media/freebox
sudo mount -t cifs //mafreebox.freebox.fr/Server\ Freebox/  /media/freebox -o user=freebox,password=mini4K,uid=1000,gid=1000,rw,sec=ntlm
tmux send -t bash.1 'rsync -aP /media/usb/photos /media/freebox/photos' ENTER
tmux send -t bash.1 'pushbullet push all note "Backup Photos" "Backup finished"' ENTER
tmux select-window -t pi:bash
tmux select-pane -t bash.1
tmux -2 attach -t pi
